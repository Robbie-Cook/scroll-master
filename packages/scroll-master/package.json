{
  "name": "scroll-master",
  "version": "1.0.9",
  "description": "A package to help fix items in a scroll container",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack --watch",
    "build": "webpack",
    "prepublishOnly": "npm run build",
    "preversion": "npm run build"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "dist/**/*.{js,css}",
            "docs",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Robbie-Cook/scroll-master.git"
  },
  "keywords": [
    "scroll",
    "master",
    "scroll-master",
    "scrollmaster"
  ],
  "author": "Robbie Cook <robbie@robbie.digital>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Robbie-Cook/scroll-master/issues"
  },
  "homepage": "https://github.com/Robbie-Cook/scroll-master#readme",
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.10",
    "lerna": "^3.22.1",
    "source-map-loader": "^1.1.0",
    "ts-loader": "^8.1.0",
    "typescript": "^4.0.3",
    "webpack": "4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  },
  "gitHead": "6eda4aac98be6ee74e7c188df250d1a1e92cc889",
  "dependencies": {
    "@types/lodash": "^4.14.162",
    "lodash": "^4.17.20"
  }
}
